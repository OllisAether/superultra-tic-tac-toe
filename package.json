{
  "name": "super-tic-tac-toe",
  "private": true,
  "version": "0.0.0",
  "type": "commonjs",
  "scripts": {
    "dev": "vite --host",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vue-tsc && vite build",
    "build:server": "esbuild ./src/server/index.ts --outfile=dist/server/index.js --bundle --platform=node",
    "preview": "vite preview",
    "serve": "npx tsx watch src/server/index.ts",
    "start": "node dist/server/index.js",
    "deploy:server": "gcloud app deploy",
    "deploy:client": "firebase deploy --only hosting",
    "gcp-build": ""
  },
  "dependencies": {
    "@types/cookie-parser": "^1.4.7",
    "cookie-parser": "^1.4.6",
    "eventsource-parser": "^1.1.2",
    "pinia": "^2.1.7",
    "vue": "^3.4.21",
    "vue-router": "^4.3.2"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@vitejs/plugin-vue": "^5.0.4",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "esbuild": "0.21.1",
    "express": "^4.19.2",
    "sass": "^1.77.0",
    "typescript": "^5.4.5",
    "vite": "^5.2.0",
    "vite-plugin-pwa": "^0.20.0",
    "vue-tsc": "^2.0.6"
  }
}
